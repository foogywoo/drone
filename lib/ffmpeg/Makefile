	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: ffmpeg.pro  /Developer/qt/mkspecs/default/qmake.conf /Developer/qt/lib/libqt-mt.prl
	$(QMAKE) -o Makefile ffmpeg.pro
qmake: 
	@$(QMAKE) -o Makefile ffmpeg.pro

ffmpeg.app/Contents/MacOS/../PkgInfo: 
	@test -d ffmpeg.app/Contents/MacOS/ || mkdir -p ffmpeg.app/Contents/MacOS/
	@$(DEL_FILE) ffmpeg.app/Contents/MacOS/../PkgInfo
	@echo "APPL????" >ffmpeg.app/Contents/MacOS/../PkgInfo
ffmpeg.app/Contents/MacOS/../Info.plist: 
	@test -d ffmpeg.app/Contents/MacOS/ || mkdir -p ffmpeg.app/Contents/MacOS/
	@$(DEL_FILE) ffmpeg.app/Contents/MacOS/../Info.plist
	@sed -e "s,@ICON@,application.icns,g" -e "s,@EXECUTABLE@,ffmpeg,g" "/Developer/qt/mkspecs/default/Info.plist.app" >"ffmpeg.app/Contents/MacOS/../Info.plist"
dist: 
	@mkdir -p .tmp/ffmpeg && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/ffmpeg/ && ( cd `dirname .tmp/ffmpeg` && $(TAR) ffmpeg.tar ffmpeg && $(GZIP) ffmpeg.tar ) && $(MOVE) `dirname .tmp/ffmpeg`/ffmpeg.tar.gz . && $(DEL_FILE) -r .tmp/ffmpeg

