all: Makefile ffmpeg.app/Contents/MacOS/../PkgInfo ffmpeg.app/Contents/MacOS/../Info.plist $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	test -d ffmpeg.app/Contents/MacOS/ || mkdir -p ffmpeg.app/Contents/MacOS/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

